{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.svg": [
      "@parcel/transformer-svg-react"
    ]
  },
  "validators": {
    "*.{css,scss}": [
      "parcel-validator-stylelint"
    ],
    "*.{ts,tsx}": [
      "@parcel/validator-typescript",
      "parcel-validator-stylelint",
      "@parcel/validator-eslint"
    ]
  }
}
